{
  "version": 2,
  "name": "openboard",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/next"
    },
    {
      "src": "./backend/**/*.ts",
      "use": "@now/node@canary"
    }
  ],
  "env": {
    "IS_NOW": "true"
  },
  "routes": [
    { "src": "/proxy/(.*)", "dest": "/backend/proxy.ts", "headers": { "Access-Control-Allow-Origin": "*" } },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
